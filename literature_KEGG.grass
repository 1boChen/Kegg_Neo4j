node {
  diameter: 50px;
  color: orange;
  border-color: #ffffff;
  border-width: 2px;
  text-color-internal: #000000;
  font-size: 10px;
  caption:"{name} ({class})";
}
relationship {
  color: black;
  shaft-width: var({credibility});
  width: var({credibility});
  font-size: 10px;
  padding: 3px;
  text-color-external: #000000;
  text-color-internal: #FFFFFF;
  caption: "{PMCID} {relation_name} ({credibility})";
}
node.* {
  color: #C990C0;
  border-color: #b261a5;
  text-color-internal: #FFFFFF;
  defaultCaption: "<id>";
}
node.gene {
  color: #00ffff;
  border-color: #000000;
  text-color-internal: #000000;
  caption: "{name}";
}
node.ortholog {
  color: #006699;
  border-color: #ffffff;
  text-color-internal: #ffffff;
  caption: "{name}";
}
node.compound {
  color: #66ff66;
  border-color: #000000;
  text-color-internal: #000000;
  caption: "{name}";
}
node.enzyme {
  color: #ffff00;
  border-color: #000000;
  text-color-internal: #000000;
  caption: "{name}";
}
node.reaction {
  color: #ff6600;
  border-color: #000000;
  text-color-internal: #000000;
  caption: "{name}";
}
node.group {
  color: #cc0066;
  border-color: #000000;
  text-color-internal: #000000;
  caption: "{name}";
}
node.brite {
  color: #660066;
  border-color: #ffffff;
  text-color-internal: #ffffff;
  caption: "{name}";
}
node.map {
  color: #993333;
  border-color: #ffffff;
  text-color-internal: #ffffff;
  caption: "{name}";
}

relationship.compound {
  color: #800000;
  shaft-width: var({credibility});
}
relationship.expression {
  color: #ff0000;
  shaft-width: var({credibility});
}
relationship.activation {
  color: #ff6699;
  shaft-width: "var({credibility})"px;
  width: "var({credibility})";
}
relationship.inhibition {
  color: #00cc66;
  shaft-width: var({credibility});
  width: "var({credibility})";
}
relationship.phosphorylation {
  color: #ffcc00;
  shaft-width: var({credibility});
}
relationship.dephosporylation {
  color: #3333ff;
  shaft-width: var({credibility});
}
relationship.glycosylation {
  color: #cc6699;
  shaft-width: var({credibility});
}
relationship.ubiquitination {
  color: #9900cc;
  shaft-width: var({credibility});
}
relationship.methylation {
  color: #33cccc;
  shaft-width: var({credibility});
}
relationship.indirectEffect {
  color: #666699;
  shaft-width: var({credibility});
}
relationship.hiddenCompound {
  color: #669999;
  shaft-width: var({credibility});
}
relationship.stateChange {
  color: #00cc00;
  shaft-width: var({credibility});
}
relationship.binding/association {
  color: #ff9933;
  shaft-width: var({credibility});
}
relationship.dissociation {
  color: #0033cc;
  shaft-width: var({credibility});
}
relationship.missingInteraction {
  color: #000066;
  shaft-width: var({credibility});
}
relationship.has_alia {
  color: blue;
  shaft-width: var({credibility});
  width: var({credibility});
  font-size: 10px;
  padding: 3px;
  text-color-external: #000000;
  text-color-internal: #FFFFFF;
  caption: "{type}";
}
